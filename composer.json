{
    "name": "zagacha/huseynjon",
    "description": "zadacha",
    "type": "project",
    "config": {
        "platform": {
            "php": "7.3"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phan/phan": "^1.2",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11.2",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=PSR2 -n src",
        "phpmd": "phpmd src text cleancode,codesize,controversial,design,naming,unusedcode",
        "phan": "phan --allow-polyfill-parser",
        "psalm": "psalm",
        "phpstan": "phpstan analyse src tests --level 3",
        "lint": [
            "@phpcs",
            "@phpmd"
        ]
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "huseynjon",
            "email": "husanjan2019@gmail.com"
        }
    ]
}
